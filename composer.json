{
    "name": "prgrssm/simple-crud",
    "type": "project",
    "description": "Simple crud for laravel",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/framework": ">=10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SimpleCrud\\Providers\\CrudProvider"
            ],
            "aliases": {
                "CrudRoutes": "SimpleCrud\\Facade\\CrudRoutesBinderFacade"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "SimpleCrud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleCrud\\Tests\\": "tests/"
        }
    }
}
