{
    "name": "prgrssm/simple-crud",
    "type": "project",
    "description": "Simple crud for laravel",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^6.2",
        "laravel/tinker": "^2.0"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SimpleCrud\\Providers\\CrudProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "SimpleCrud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleCrud\\Tests\\": "tests/"
        }
    }
}
